{
  "name": "tinged",
  "productName": "Tinged",
  "version": "0.2.1",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Jeremy Allard",
    "email": "jeremy@allard.io"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tinged"
          },
          "platforms": [
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.49",
    "@electron-forge/maker-deb": "^6.0.0-beta.49",
    "@electron-forge/maker-rpm": "^6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.49",
    "@electron-forge/maker-zip": "^6.0.0-beta.49",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.49",
    "@hot-loader/react-dom": "^16.11.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/debounce": "^1.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/howler": "^2.1.2",
    "@types/palx": "^1.0.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^4.4.2",
    "@types/styled-system": "^5.1.7",
    "@types/use-resize-observer": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "css-loader": "^3.4.2",
    "electron": "11.5.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@electron-forge/maker-pkg": "^6.0.0-beta.49",
    "@use-it/event-listener": "^0.1.3",
    "debounce": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "howler": "^2.1.3",
    "music-metadata-browser": "^2.0.3",
    "palx": "^1.0.3",
    "polished": "^3.4.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.19",
    "react-icons": "^3.9.0",
    "react-spring": "^8.0.27",
    "react-use": "^13.24.0",
    "react-use-promise": "^0.2.0",
    "styled-components": "^5.0.1",
    "styled-media-query": "^2.1.2",
    "styled-system": "^5.1.5",
    "ts-key-enum": "^2.0.2",
    "typeface-aileron": "0.0.66",
    "use-onclickoutside": "^0.3.1",
    "use-resize-observer": "^6.0.0"
  }
}
